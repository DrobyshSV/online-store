{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
    "plugins": ["prettier", "import"],
    "extends": [
        "plugin:prettier/recommended",
        "prettier"
    ],

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off"
    }
}
